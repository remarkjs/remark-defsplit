{
  "name": "remark-defsplit",
  "version": "1.2.0",
  "description": "Extract inline link/image destinations as separate definitions",
  "license": "MIT",
  "keywords": [
    "definition",
    "destination",
    "extract",
    "full",
    "http",
    "https",
    "image",
    "link",
    "markdown",
    "mdast",
    "plugin",
    "reference",
    "remark",
    "split",
    "target",
    "uri",
    "url"
  ],
  "repository": "eush77/remark-defsplit",
  "bugs": "https://github.com/eush77/remark-defsplit/issues",
  "author": "Eugene Sharygin <eush77@gmail.com>",
  "contributors": [
    "Eugene Sharygin <eush77@gmail.com>",
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "unist-util-index": "^1.0.0"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "remark": "^9.0.0",
    "remark-cli": "^5.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "tape": "^4.0.2",
    "xo": "^0.22.0"
  },
  "scripts": {
    "format": "remark *.md -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test/test",
    "test": "npm run format && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
